.banner{
  width: 100%;
  height: 380rpx;
}
.banner image{
  width: 100%;
  height: 100%;
}
.nav{
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  border-top: 1px solid pink;
  border-left: 1px solid pink;
}

.nav .nav-item{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 33.3333%;
  height: 250rpx;
  box-sizing: border-box;
  border-right: 1px solid pink;
  border-bottom: 1px solid pink;
}

.nav image{
  width: 70rpx;
  height: 70rpx;
}


.footer{
  display: flex;
  margin-top: 10rpx;
}
.footer image{
  width: 375rpx;
  height: 250rpx;
}